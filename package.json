{
  "name": "personal-website",
  "private": true,
  "version": "0.0.1",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "eleventy:default": "npx eleventy",
    "eleventy:watch": "npx eleventy --watch --incremental",
    "serve": "light-server -c .lightserverrc",
    "dev": "NODE_ENV=dev npm-run-all clean eleventy:default --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "build": "NODE_ENV=prod run-s clean eleventy:default --print-label"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adueppen/personal-website.git"
  },
  "author": "Alex Dueppen",
  "license": "SEE LICENSE IN README.md",
  "bugs": {
    "url": "https://github.com/adueppen/personal-website/issues"
  },
  "homepage": "https://github.com/adueppen/personal-website#readme",
  "alias": {
    "assets": "./src/assets",
    "includes": "./src/includes",
    "data": "./src/data"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@aloskutov/eleventy-plugin-external-links": "^1.4.0",
    "@csstools/postcss-sass": "^5.0.1",
    "@piraces/eleventy-plugin-pwa": "^1.1.2",
    "@tsconfig/recommended": "^1.0.1",
    "autoprefixer": "^10.4.4",
    "browserify": "^17.0.0",
    "cssnano": "^5.1.7",
    "html-minifier": "^4.0.0",
    "light-server": "^2.9.1",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.3",
    "stream-to-string": "^1.2.0",
    "tsify": "^5.0.4",
    "typescript": "^4.6.3",
    "uglifyify": "^5.0.2",
    "workbox-window": "^6.5.2"
  }
}
